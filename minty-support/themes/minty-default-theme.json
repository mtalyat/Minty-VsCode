{
	"name": "Minty",
	"colors": {
		// window
		"activityBar.background": "#003000", // activity bar on the left
		"tab.inactiveBackground": "#101510", // inactive tabs
		"tab.activeBackground": "#131", // active (selected) tab
		"tab.lastPinnedBorder": "#00ff0044", // last pinned tab border
		"sideBar.background": "#002000", // sidebar (explorer) background
		"statusBar.background": "#005000", // status bar at the bottom
		"statusBar.noFolderBackground": "#005000", // status bar when no folder is open
		"statusBarItem.remoteBackground": "#3c3", // remote status bar item background
		"ports.iconRunningProcessForeground": "#58db7d", // foreground color for running process icons
		"editorGroupHeader.tabsBackground": "#001000", // background for editor group header tabs
		"titleBar.activeBackground": "#004000", // active title bar background
		"titleBar.inactiveBackground": "#254425", // inactive title bar background
		"selection.background": "#00aaff77", // selection background color window text selection (renaming files, etc.)
		// editor
		"editor.background": "#001000", // file background
		"editorGroup.border": "#66ff6644", // file border
		"editorCursor.foreground": "#0a0", // cursor
		"editor.foreground": "#F8F8F8", // text
		"editorWhitespace.foreground": "#387f38", // scope lines
		"editor.selectionBackground": "#004e75", // active selection highlight
		"minimap.selectionHighlight": "#004e75", // minimap active selection highlight
		"editorLineNumber.foreground": "#77ff7788", // line numbers
		"editorLineNumber.activeForeground": "#77ff77dd", // active line number
		"editorWidget.background": "#030", // find/replace box color
		"editorHoverWidget.background": "#030", // hover over element box color
		"editorSuggestWidget.background": "#030", // inactive autocomplete box item color
		"editorSuggestWidget.border": "#060", // autocomplete box border
		"editor.lineHighlightBackground": "#00aaff33", // active line background
		"editor.hoverHighlightBackground": "#00557f", // hover over element highlight color
		"editor.selectionHighlightBackground": "#00aaff20", // highlight for selected text copies in file
        "editor.wordHighlightBackground": "#00aaff20", // highlight for word under cursor
        "editor.wordHighlightStrongBackground": "#00aaff20", // strong highlight for word under cursor
		"editorLink.activeForeground": "#00ff00aa", // link when holding ctrl
		"peekViewTitle.background": "#005500", // peek view header background
		"peekView.border": "#00ff0044", // peek view border
		"peekViewResult.background": "#131", // peek view list background
		"peekViewEditor.background": "#121", // peek view file background
		// UI
		"debugToolBar.background": "#060", // debug toolbar background
		"focusBorder": "#66ff66aa", // focus border color
		"button.background": "#282", // button background color
		"dropdown.background": "#141", // dropdown and checkbox background color
		"input.background": "#141", // input field background color
		"inputOption.activeBorder": "#00cc00", // ?
		"inputValidation.infoBackground": "#005500", // ?
		"inputValidation.infoBorder": "#006600", // ?
		"list.hoverBackground": "#070", // hover background for list items
		"list.activeSelectionBackground": "#090", // active selection background for list items
		"list.inactiveSelectionBackground": "#080", // inactive selection background for list items
		"list.dropBackground": "#226622", // background color for list items when dragging
		"quickInputList.focusBackground": "#006600", // selected item in quick input list (F1)
		"list.highlightForeground": "#66dd66", // quick input list filter text color
		"pickerGroup.foreground": "#99cc99", // quick input list group text color
		"pickerGroup.border": "#00ff0033", // quick input list group border color
		"badge.background": "#33aa33", // badge background color (e.g., for notifications, source control number, etc.)
		"progressBar.background": "#33aa33", // progress bar color
		"errorForeground": "#eaffea", // ?
		"extensionButton.prominentBackground": "#33aa33", // ?
		"extensionButton.prominentHoverBackground": "#33aa3388" // ?
	},
	"tokenColors": [
